{
    "name": "buig",
    "version": "0.0.6",
    "description": "buig(big + bug + ui) - 全是 BUG 的 Vue3 UI组件库",
    "type": "module",
    "private": false,
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org"
    },
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        },
        "./components/*": {
            "import": "./dist/components/*.js",
            "types": "./dist/components/*.d.ts"
        },
        "./styles/*": "./dist/styles/*"
    },
    "types": "./dist/index.d.ts",
    "sideEffects": [
        "**/*.css"
    ],
    "scripts": {
        "build": "vite build",
        "dev": "vite",
        "test": "vitest run",
        "test:watch": "vitest",
        "e2e": "playwright test",
        "docs:dev": "vitepress dev docs",
        "docs:build": "vitepress build docs",
        "docs:preview": "vitepress preview docs",
        "gen:meta": "bun run-ts scripts/genComponentMeta.ts"
    },
    "keywords": [
        "bun",
        "ui",
        "framework",
        "javascript"
    ],
    "author": "chensuiyi <bimostyle@qq.com>",
    "homepage": "https://chensuiyi.me",
    "license": "Apache-2.0",
    "files": [
        "dist/",
        "LICENSE",
        "package.json",
        "README.md"
    ],
    "devDependencies": {
        "@vitejs/plugin-vue": "^6.0.1",
        "vite": "^7.1.7",
        "vitepress": "^2.0.0-alpha.12",
        "vue": "^3.5.22",
        "typescript": "^5.4.0",
        "vue-tsc": "^3.0.8",
        "@vue/compiler-sfc": "^3.5.22"
    }
}
